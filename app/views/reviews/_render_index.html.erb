<% @reviews.each do |review| %>
  <div class="card-review m-2">
    <div class="card-body">
      <p class="card-text flex-inline">
        <% review.rating.times do %>
          <i class="fa-solid fa-star text-info"></i>
        <% end %>
      </p>
      <p class="card-text"> <%= review.content %></p>
    </div>
  </div>
<% end %>
